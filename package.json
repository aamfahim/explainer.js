    "bugs": {
        "url": "https://github.com/aamfahim/explainer.js/issues"
    },
    "homepage": "https://github.com/aamfahim/explainer.js#readme",
    "dependencies": {
        "@ltd/j-toml": "^1.38.0",
        "commander": "^12.1.0",
        "dotenv": "^16.4.5",
        "groq-sdk": "^0.7.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "eslint": "^9.13.0",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "3.3.3"
    }
